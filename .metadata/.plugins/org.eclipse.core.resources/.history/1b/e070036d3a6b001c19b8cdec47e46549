package ListAssigment;

public class SinglyLinkedList {
	
    public Node head;
    
    //constructor head
    public SinglyLinkedList(Node cabeza) {	
    	cabeza = head;
    }


	//Metodo remove
    public void remove(int value) {   	
        }
    
    //Metodo printValues
    public void printValues() {
    	
    }
    
    
    public void add(int value) {

        Node newNode = new Node(value);

        if(head == null) {
            head = newNode;

        } else {

            Node runner = head;
            while(runner.next != null) {
                runner = runner.next;
            }
            
            runner.next = newNode;

        }
    } 

}
