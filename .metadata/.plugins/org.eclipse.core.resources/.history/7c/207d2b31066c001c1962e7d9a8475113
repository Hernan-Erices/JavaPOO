package maestroDeObjetos2;

public class Human{
	
	private int strength;
	private int stealth;
	private int intelligence;
	private int health;
	

	
	public Human() {
		strength = 0; // fuerza
		stealth = 0; // sigilo
		intelligence = 0; //inteligencia
		health = 0; //salud
	}
	
	
	//GETTERS
	
	public int getStrength() {
		return strength;
	}
	
	public int getStealth() {
		return stealth;
	}
	
	public int GetIntelligence() {
		return intelligence;
	}
	
	public int GetHealth() {
		return health;
	}
	
	//Agregar el método attack
	
	}
